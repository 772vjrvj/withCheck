<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<!--mybatis할 때 1.환경설정파일 config -->
<!-- 		   2.쿼리작성파일 mapper-->
<configuration>
<!-- 	sql_mapper.xml에서 사용하게 될 결과나 파라미터를 담을 객체를 정의 -->
	<typeAliases>
		<typeAlias type="com.hk.toCheckFinal.dtos.HcDto" alias="HcDto"/>
		<typeAlias type="com.hk.toCheckFinal.dtos.HcLoginDto" alias="HcLoginDto"/>		
		<typeAlias type="com.hk.toCheckFinal.dtos.HcChkDto" alias="HcChkDto"/>
		<typeAlias type="com.hk.toCheckFinal.dtos.HcInChkDto" alias="HcInChkDto"/>		
	</typeAliases>
	
<!-- 쿼리를 작성할 xml파일 위치 설정	 -->
	<mappers>
		<mapper resource="/sqls/Hc_mapper.xml"/>
	</mappers>
</configuration>